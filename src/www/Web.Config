			<add verb="*" path="blogml.axd" type="CoyoEden.Core.Web.HttpHandlers.BlogMLExportHandler, CoyoEden.Core" validate="false"/>
			<add verb="*" path="sioc.axd" type="CoyoEden.Core.Web.HttpHandlers.Sioc, CoyoEden.Core" validate="false"/>
